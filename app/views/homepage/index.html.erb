<h1 id = "title">Class Champion</h1>
<div class ="card card-default" id=sign-in-box>
  <div id=sign-in-box-heading class="card-heading">Sign-in</div>
  <div class = "card-body">
<form>
  <div class="form-group">
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Email Address">
  </div>
  <div class="form-group">
    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
  </div>
  <button type="submit" class="sign-in-btn">Submit</button>
</form>
</div>
</div>
 <h1>OR</h1>
 
  <%= link_to t(".sign_up"), sign_up_path,:class => "btn btn-primary homepage-btn" %>
